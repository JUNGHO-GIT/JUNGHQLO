{
  "$schema": "https://json.schemastore.org/jsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "typeRoots": [
      "./src/main/resources/static/type"
    ],
    "types": [
      "global"
    ]
  },
  "include": [
    "src/**/*",
    "**/*.ts",
    "**/*.tsx",
    "**/*.d.ts",
    "**/*.js",
    "**/*.jsx"
  ],
  "exclude": [
    "target/**/*",
  ]
}