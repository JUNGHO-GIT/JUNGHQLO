{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/tmp": true,
    "**/.mvn": true,
    "**/mvnw": true,
    "**/mvnw.cmd": true,
    "**/**.git": true,
    "**/**.svn": true,
    "**/**.hg": true,
    "**/**.DS_Store": true,
    "**/**.mvn": true,
    "**/**.git**": true,
    "**/readme.md": true,
    "**/README.md": true,
    "**/LICENSE.md": true,
    "**/CHANGELOG.md": true,
    "**/CHANGELOG": true,
    "**/**.gitignore": true,
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/**build/**": true,
    "**/target": true,
    "build.tar.gz": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/**.git": true,
    "**/**.svn": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/build": true,
    "**/target": true,
    "**/*.conf": true,
    "**/*.json": true,
    "**/*.txt": true,
    "**/*gcloud*": true,
    "**/*Auth*": true,
    "**/*.env*": true,
  },
  "explorerExclude.backup": {},
  "files.associations": {
    "*.properties": "spring-boot-properties"
  },
  "java.configuration.updateBuildConfiguration": "automatic",
  "maven.terminal.favorites": [
    {
      "command": "mvn clean package -Pproduction",
      "debug": false
    }
  ]
}