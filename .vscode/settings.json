{
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "**/bower_components": false,
    "**/coverage": false,
    "**/tmp": false,
    "**/.mvn": false,
    "**/mvnw": false,
    "**/mvnw.cmd": false,
    "**/**.git": false,
    "**/**.svn": false,
    "**/**.hg": false,
    "**/**.DS_Store": false,
    "**/**.mvn": false,
    "**/**.git**": false,
    "**/readme.md": false,
    "**/README.md": false,
    "**/LICENSE.md": false,
    "**/CHANGELOG.md": false,
    "**/CHANGELOG": false,
    "**/**.gitignore": false,
    "**/node_modules": false,
    "**/package-lock.json": false,
    "**/**build/**": false,
    "**/target": false,
    "build.tar.gz": false
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/**.git": true,
    "**/**.svn": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/build": true,
    "**/target": true,
    "**/*.conf": true,
    "**/*.json": true,
    "**/*.txt": true,
    "**/*gcloud*": true,
    "**/*Auth*": true,
    "**/*.env*": true,
  },
  "explorerExclude.backup": {},
  "java.configuration.updateBuildConfiguration": "automatic",
  "maven.terminal.favorites": [
    {
      "command": "mvn clean package -Pproduction",
      "debug": false
    }
  ]
}